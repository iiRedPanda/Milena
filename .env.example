# Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_GUILD_ID=your_discord_guild_id
BOT_OWNERS=owner_id_1,owner_id_2
SUPPORT_SERVER=support_server_invite_link
BOT_INVITE_URL=bot_invite_url

# API Keys
GEMINI_API_KEY=your_gemini_api_key
GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta/models/gemini-2.5-pro-exp-03-25:generateContent
GOOGLE_API_KEY=your_google_api_key
WEATHER_API_KEY=your_weather_api_key

# Environment
NODE_ENV=development # or production
LOG_LEVEL=info # debug, info, warn, error
VERBOSE_LOGGING=false

# Performance Settings
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_MAX_SIZE=1000

# Storage Configuration
STORAGE_TYPE=local # or s3
AWS_ACCESS_KEY_ID=your_aws_access_key # if using S3
AWS_SECRET_ACCESS_KEY=your_aws_secret_key # if using S3
AWS_REGION=your_aws_region # if using S3
S3_BUCKET=your_s3_bucket # if using S3

# Monitoring
METRICS_ENABLED=true
ANALYTICS_ENABLED=true

# Feature Flags
GAMES_ENABLED=true
AI_ENABLED=true
MODERATION_ENABLED=true
AUTOMOD_ENABLED=true

# Development
DEBUG_ENABLED=false
COMMAND_TIMING=true
